import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner dados = new Scanner(System.in);

    int n;
    int mult=1;

    System.out.print("Informe o n√∫mero desejado: ");
    n = dados.nextInt();

    do{
      System.out.println(n*mult);
      mult++;
    }while(mult<=10);
    
  }
}
